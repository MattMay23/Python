"""podaj_bok1 = input("Podaj pierwszy bok: ")
podaj_bok2 = input("Podaj drugi bok: ")
podaj_bok3 = input("Podaj trzeci bok: ")
if(int(podaj_bok1) > 0 and int(podaj_bok2) > 0 and int(podaj_bok3) > 0):
    if int(podaj_bok1) > int(podaj_bok2):
        if int(podaj_bok1) > int(podaj_bok3):
            if int(podaj_bok2) != int(podaj_bok3):
                kwadrat_dluzszego = int(podaj_bok1) * int(podaj_bok1)
                kwadrat_krotszego1 = int(podaj_bok2) * int(podaj_bok2)
                kwadrat_krotszego2 = int(podaj_bok3) * int(podaj_bok3)
                suma_kwadratow = kwadrat_krotszego1 + kwadrat_krotszego2
                if(suma_kwadratow == kwadrat_dluzszego):
                    print("Trójkąt JEST prostokątny")
                else:
                    print("Trójkąt NIE JEST prostokątny")
            else:
                    print("Trójkąt NIE JEST prostokątny")

    if int(podaj_bok2) > int(podaj_bok1):
        if int(podaj_bok2) > int(podaj_bok3):
            if int(podaj_bok1) != int(podaj_bok3):
                kwadrat_dluzszego = int(podaj_bok2) * int(podaj_bok2)
                kwadrat_krotszego1 = int(podaj_bok1) * int(podaj_bok1)
                kwadrat_krotszego2 = int(podaj_bok3) * int(podaj_bok3)
                suma_kwadratow = kwadrat_krotszego1 + kwadrat_krotszego2
                if(suma_kwadratow == kwadrat_dluzszego):
                    print("Trójkąt JEST prostokątny")
                else:
                    print("Trójkąt NIE JEST prostokątny")
            else:
                print("Trójkąt NIE JEST prostokątny")

    if int(podaj_bok3) > int(podaj_bok1):
        if int(podaj_bok3) > int(podaj_bok2):
            if int(podaj_bok1) != int(podaj_bok2):
                kwadrat_dluzszego = int(podaj_bok3) * int(podaj_bok3)
                kwadrat_krotszego1 = int(podaj_bok1) * int(podaj_bok1)
                kwadrat_krotszego2 = int(podaj_bok2) * int(podaj_bok2)
                suma_kwadratow = kwadrat_krotszego1 + kwadrat_krotszego2
                if(suma_kwadratow == kwadrat_dluzszego):
                    print("Trójkąt JEST prostokątny")
                else:
                    print("Trójkąt NIE JEST prostokątny")
            else:
                print("Trójkąt NIE JEST prostokątny")

    if int(podaj_bok1) == int(podaj_bok2) and int(podaj_bok2) == int(podaj_bok3):
        print("Trójkąt NIE JEST prostokątny")
    elif int(podaj_bok1) == int(podaj_bok2) and int(podaj_bok2) > int(podaj_bok3):
        print("Trójkąt NIE JEST prostokątny")

    elif int(podaj_bok1) == int(podaj_bok3) and int(podaj_bok3) > int(podaj_bok2):
        print("Trójkąt NIE JEST prostokątny")
    elif int(podaj_bok2) == int(podaj_bok3) and int(podaj_bok3) > int(podaj_bok1):
        print("Trójkąt NIE JEST prostokątny")
else:
    print("Trójkąt NIE JEST prostokątny")"""
